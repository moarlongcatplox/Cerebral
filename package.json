{
  "name": "eve-cerebral-fork",
  "productName": "Cerebral Fork",
  "version": "0.0.1",
  "description": "Character Manager for EVE Online",
  "author": "Needmore Longcat forked from Prometheus Satyen",
  "license": "AGPL-3.0-only",
  "repository": "github:moarlongcatplox/Cerebral",
  "keywords": [],
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --cache --color --ext .jsx,.js src"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm",
        "icon": "resources/icon.ico",
        "ignore": [
          ".idea"
        ]
      },
      "electronWinstallerConfig": {
        "name": "eve_cerebral",
        "icon": "resources/icon.ico",
        "setupIcon": "resources/icon.ico",
        "loadingGif": "resources/installing.gif"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "windowsStoreConfig": {
        "packageName": "",
        "name": "EVE Cerebral"
      }
    }
  },
  "dependencies": {
    "@devexpress/dx-react-core": "^2.6.3",
    "@devexpress/dx-react-grid": "^2.6.3",
    "@material-ui/core": "^4.10.0",
    "@material-ui/icons": "^4.9.1",
    "@types/electron-store": "^3.2.0",
    "acorn": "^7.2.0",
    "electron-log": "^4.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^5.1.1",
    "frameless-titlebar": "^2.1.3",
    "material-ui": "^0.20.2",
    "node": "^14.3.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dnd": "^11.1.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-sortable-hoc": "^1.11.0",
    "react-table": "^7.1.0",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.8",
    "sanitize-html": "^1.24.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@material-ui/codemod": "^4.5.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron": "^9.0.0",
    "electron-devtools-installer": "^3.0.0",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "8.2.0",
    "electron-reload": "^1.5.0",
    "eslint": "^7.1.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0"
  }
}
